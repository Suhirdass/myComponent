    <lightning:spinner variant="brand" alternativeText="{!v.processingMessage}" class="{!(v.isProcessing? 'slds-show slds-is-fixed': 'slds-hide')}" />

    
    <div class="">
        
        <aura:if isTrue="{!v.isOpenprint}">
            <!--Modal Box Start-->
            <div role="dialog" class="slds-modal slds-fade-in-open ">
                <div class="slds-modal__container">
                    
                    
                    <!--Modal Box Header-->
                    <div class="slds-modal__content slds-p-around--medium">
                        <article class="slds-card " >
                            <div class="slds-page-header heightfixprint" role="banner">
                                <div class="slds-grid">
                                    <div class="slds-col slds-has-flexi-truncate">
                                        <div class="slds-media">
                                            <lightning:icon iconName="custom:custom29" title="custom29" />
                                            <div class="slds-media__body">
                                                
                                                <div class="slds-page-header__name">
                                                    <div class="slds-page-header__name-title">
                                                        <h1>
                                                            <span>Products</span>
                                                            <span class="slds-page-header__title slds-truncate" title="Print Label">Print Label</span>
                                                        </h1>
                                                    </div>
                                                </div>
                                            </div>	
                                        </div>
                                    </div>
                                    <div class="slds-col slds-no-flex slds-align-bottom">
                                        <div class="slds-button-group" role="group">
                                            
                                            <lightning:button class="textCap" label="Preview" onclick="{!c.updatePreview}"/>
                                            <lightning:button class="textCap"  label="Print Label" onclick="{!c.printLabel}"/>
                                        </div>
                                    </div>
                                </div>
                                
                            </div>
                            <div class="slds-card__header ">
                                
                                <div >
                                    <table class="slds-table">
                                        <thead ></thead>
                                        <tbody>
                                            <tr>
                                                <td class="slds-float_left lay" style="width:175px;bottom:-5px;"><b>Please select Dymo Label</b></td>
                                                <td class="slds-float_left"> 
                                                    <lightning:select name="a_opt3" aura:id="a_opt3" value="{!v.prodRec.selectedDymoLabelLayout}">
                                                        <aura:iteration items="{!v.prodRec.dymoLabelLayouts}" var="proTitle">
                                                            <option value="{!proTitle.label}" selected="{!proTitle.label == v.prodRec.selectedDymoLabelLayout}">{!proTitle.value}</option>
                                                        </aura:iteration>
                                                    </lightning:select>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="slds-float_left"  style="width:175px;bottom: -4px;"><b>List of Dymo Printers </b></td>
                                                <td class="slds-float_left"> 
                                                    <lightning:select name="a_opt3" aura:id="a_opt3" value="{!v.printers.selectedPrinter}">
                                                        <option value="">Select Printer</option>
                                                        <aura:iteration items="{!v.printers}" var="p">
                                                            <option value="{!p.label}" >{!p.value}</option>
                                                        </aura:iteration>
                                                    </lightning:select>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="slds-float_left"  style="width:175px;bottom: -4px;"><b>Print Qty </b></td>
                                                <td class="slds-float_left"> 
                                       <lightning:input  name="inputDate" value="{!v.noofcopies}"  />
 
                                                </td>
                                            </tr>

                                        </tbody>
                                    </table>
                                    <img src="{!v.imagetext}"/>
                                </div>
                                
                                
                            </div>  
                        </article>  
                        
                    </div>
                    <!--Modal Box Button-->
                    <div class="slds-modal__footer">
                        <button class="slds-button slds-button--brand textCap"  onclick="{!c.handleCloseModal}">Cancel</button>
                    </div>
                </div>
            </div>
            <div class="slds-backdrop slds-backdrop--open"></div>            
        </aura:if>
    </div>
    
   
   <div class="">
        
        <aura:if isTrue="{!v.isOpenprintQR}">
            <!--Modal Box Start-->
            <div role="dialog" class="slds-modal slds-fade-in-open ">
                <div class="slds-modal__container">
                    
                    
                    <!--Modal Box Header-->
                    <div class="slds-modal__content slds-p-around--medium">
                        <article class="slds-card " >
                            <div class="slds-page-header heightfixprint" role="banner">
                                <div class="slds-grid">
                                    <div class="slds-col slds-has-flexi-truncate">
                                        <div class="slds-media">
                                            <lightning:icon iconName="custom:custom29" title="custom29" />
                                            <div class="slds-media__body">
                                                
                                                <div class="slds-page-header__name">
                                                    <div class="slds-page-header__name-title">
                                                        <h1>
                                                            <span>Products</span>
                                                            <span class="slds-page-header__title slds-truncate" title="Print Label">Print QR Code</span>
                                                        </h1>
                                                    </div>
                                                </div>
                                            </div>	
                                        </div>
                                    </div>
                                    <div class="slds-col slds-no-flex slds-align-bottom">
                                        <div class="slds-button-group" role="group">
                                            
                                            <lightning:button class="textCap" label="Preview" onclick="{!c.updatePreviewQR}"/>
                                            <lightning:button class="textCap"  label="Print QR Code" onclick="{!c.printLabelQR}"/>
                                            
                                        </div>
                                    </div>
                                </div>
                                
                            </div>
                            <div class="slds-card__header ">
                                
                                <div >
                                    <table class="slds-table">
                                        <thead ></thead>
                                        <tbody>
                                            <tr>
                                                <td class="slds-float_left lay" style="width:175px;bottom:-5px;"><b>Please select Dymo Label</b></td>
                                                <td class="slds-float_left"> 
                                                    <lightning:select name="a_opt3" aura:id="a_opt3" value="{!v.Twobyfour}">
                                                         <option value="2 X 4">2 X 4</option>
                                                        <!--<aura:iteration items="{!v.prodRec.dymoLabelLayouts}" var="proTitle">
                                                            
                                                            <option value="{!proTitle.label}" selected="{!proTitle.label == v.prodRec.selectedDymoLabelLayout}">{!proTitle.value}</option>
                                                        </aura:iteration>-->
                                                    </lightning:select>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td class="slds-float_left"  style="width:175px;bottom: -4px;"><b>List of Dymo Printers </b></td>
                                                <td class="slds-float_left"> 
                                                    <lightning:select name="a_opt3" aura:id="a_opt3" value="{!v.printers.selectedPrinter}">
                                                        <option value="">Select Printer</option>
                                                        <aura:iteration items="{!v.printers}" var="p">
                                                            <option value="{!p.label}" >{!p.value}</option>
                                                        </aura:iteration>
                                                    </lightning:select>
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                    <img src="{!v.imagetext}"/>
                                </div>
                                
                                
                            </div>  
                        </article>  
                        
                    </div>
                    <!--Modal Box Button-->
                    <div class="slds-modal__footer">
                        <button class="slds-button slds-button--brand textCap"  onclick="{!c.handleCloseModal}">Cancel</button>
                    </div>
                </div>
            </div>
            <div class="slds-backdrop slds-backdrop--open"></div>            
        </aura:if>
    </div>   
    <aura:if isTrue="{!v.isOpen}">
        <!--###### MODAL BOX Start######--> 
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1"  class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container" >
                <!-- ###### MODAL BOX HEADER Start ######-->
                <header class="slds-modal__header" style="padding-top: 20px; width: 630px;">
              <span style="height: 62px !important;width: 15px !important;background-color: white !important;z-index: 2;margin-top: -14px;margin-right: -20px;"></span>

                    <lightning:buttonIcon iconName="utility:close" onclick="{! c.closeModel }" alternativeText="close"
                                          variant="bare-inverse" class="slds-modal__close"/>
                    <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate">Merge IP</h2>
                </header>
       <span style="height:14px !important;width: 35px !important;background-color: white !important;z-index: 2;margin-top: -14px;margin-right: -270px;"></span>

                <!--###### MODAL BOX BODY Part Start######-->
             <span style="height:14px !important;margin-left:613px;;width:14x`px !important;background-color: white !important;z-index: 2;margin-top: -14px;margin-right:270px;"></span>

                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1" style="width: 630px;">

                    <p class="slds-m-left_xx-large" style="margin-left: 26px;">You won't be able to revert this! Are you sure?</p>
 
                </div>

                <!--###### MODAL BOX FOOTER Part Start ######-->
                
                <footer class="slds-modal__footer"  style="width: 630px; height: 60px; z-index: 2; display:inline-flex !important;">
               <span style="height: 65px !important;width: 30px !important;background-color: white !important;z-index: 2;margin-top: -14px;margin-left: -16px;"></span>
                    <span style="height: 50px !important; width: 560px  !important; z-index: 1;">
                    <lightning:button class="slds-button_brand textCap"  label="Yes" title="Yes"  onclick="{! c.likenClose }"/>
                    <lightning:button class="slds-button_neutral textCap"  label="No" title="No"  onclick="{! c.closeModel }"/>
                    </span>
                    <span style="height: 65px !important;background-color: white !important;z-index: 2;margin-top: -14px;margin-right: -20px;"></span>
                </footer>
                 <span style="height: 67px !important;background-color: white !important;z-index: 2;margin-top: -64px; width: 12px;margin-right: -40px;margin-left: 618px;"></span>


                <div style="padding-bottom: 60px;height:56px; width: 630px; background-color: white; border-radius: 3px; margin-top: -5px; z-index: 1;"></div>
            </div>

        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
    
       
<article class="slds-card " style="margin-top:0 !important;">
<div class="slds-page-header heightfix" role="banner">
  <div class="slds-grid">
    <div class="slds-col slds-has-flexi-truncate">
      <div class="slds-media">
        <lightning:icon iconName="custom:custom83" title="custom83" />
        <div class="slds-media__body">
          <div class="slds-grid">
           <h1 class="slds-page-header__title slds-m-rightsmall slds-truncate slds-align-middle title-align" title="Record Title">Product Details</h1> 
            </div>
        </div>	
      </div>
    </div>
       <div class="slds-col slds-no-flex slds-align-bottom">
      <div class="slds-button-group" role="group">
          <aura:if isTrue="{! v.prodRec.miscIssueSectionRender}">
   <lightning:button class="slds-button slds-button--neutral textCap" label="Save" onclick="{!c.onSave}"/>     
   <lightning:button class="slds-button slds-button--neutral textCap" label="Cancel" onclick="{!c.onCancel}"/> 
  </aura:if> 
    <aura:if isTrue="{!or(v.prodRec.showMassTransferBlock, v.prodRec.miscIssueSectionRender)}">
    <aura:set attribute="else">
      <lightning:button class="slds-button slds-button--neutral textCap" label="Item Detail" onclick="{!c.redirectItemDetail}"/>
      <lightning:button class="slds-button slds-button--neutral textCap" label="Cancel" onclick="{!c.onCancel}"/>   
    </aura:set>
  </aura:if> 
          <aura:if isTrue="{! v.prodRec.showMassTransferBlock}">
           <lightning:button class="slds-button slds-button--neutral textCap" onclick="{!c.submitTransfer}" label="Submit Transfer"/>
          <lightning:button class="slds-button slds-button--neutral textCap" label="Cancel" onclick="{!c.onCancel}"/> 
          </aura:if> 
    
      </div>
    </div>
  </div>
</div>

    <br/>
    <div class="slds-is-relative slds-p-horizontal_small">
       <div class="slds-grid slds-wrap slds-text-align_center custom-container">         
            

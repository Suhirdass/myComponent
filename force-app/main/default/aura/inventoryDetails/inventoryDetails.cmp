<aura:component extends="c:utility" controller="InventoryDetails" implements="force:lightningQuickActionwithoutheader,force:lightningQuickAction,force:hasRecordId,flexipage:availableForAllPageTypes,forceCommunity:availableForAllPageTypes" access="global">
	
    <aura:handler name="init" action="{!c.doInit}" value="{!this}" access="global" />	
    <aura:attribute name="prodRec" type="Object"/>
    <aura:attribute name="searchStr" type="String"/>
    <aura:attribute name="inventoryList" type="InventoryPositionWrapper[]"/>
    <aura:attribute name="isOpen" type="boolean" default="false"/>
    <aura:attribute name="isScriptsLoaded" type="boolean" default="false"/>
    <aura:attribute name="isDataLoaded" type="boolean" default="false"/>
    <aura:attribute name="recordId" type="String" />
    <aura:attribute name="limit" type="Integer" default="10" />
    <aura:attribute name="sortOrder" type="String" default=" ASC "/>
    <aura:attribute name="space" type="String" default="       "/>
    <aura:attribute name="inventoryPositionByIdLight" type="Object" />
     <aura:attribute name="selectedDymoLabelLayout" type="String" default=''/>
    <aura:attribute name="selectedPrinter" type="String" default=''/>
    <aura:attribute name="imageXml" type="String" default=''/>
    <aura:attribute name="printers" type="List" default='[]'/>
    <aura:attribute name="imagetext" type="String" default=''/>
    <aura:attribute name="isSelectedInventory" type="Boolean"/>
     <aura:attribute name="isOpenprint" type="boolean" default="false"/>
     <aura:attribute name="isOpenprintQR" type="boolean" default="false"/>
    <aura:attribute name="Twobyfour" type="string"/>
    <aura:attribute name="noofcopies" type="string"/>

   <ltng:require styles="{!join(',',$Resource.jQueryDataTablesZip+'/css/jquery.dataTables.css')}" scripts="{!join(',',$Resource.SweetAlert,$Resource.datatable+ '/js/jquery.min.js',
                           $Resource.datatable+ '/js/jquery.dataTables.min.js',$Resource.DymoFramework)}" afterScriptsLoaded="{!c.onScriptsLoaded}"/>
    
    <aura:html tag="style">
        .cUtility button {
        border-radius: 4px !important;
        } 
        table {
        border-collapse: unset; 
        /* border-spacing: 0; */
        }
        .textCap {
        text-transform: capitalize !important; 
        }
        .slds-card__header {
        
        margin-bottom: -25px;
        }  
        .slds-page-header__name h1 {
        margin-left: 8px;
        }
        .slds-table td {
        width: 172px;
        }
        .slds-modal__container {
        min-width: 35%;
        left: 100px;
        }
        .slds-form-element__label {
        display : none;
        }
        .back {
        background-color: white;
        }
        .dataTables_length{
        margin-bottom:7px;
        }
        .slds-card
        {
        margin-top: 9px;
        margin-left: 17px;
        margin-right: 17px;
        margin-bottom: 14px;
        padding-bottom: 10px;
        }
        .title-align {
        padding-left: 10px;
        padding-top: 5px;
        }
        .title-align-two {
        padding-left: 10px;
        }
        .title-align-two li{
        padding-bottom: 0px;
        }
        .proDet{
        margin-bottom: 10px;
        margin-left: 20px;
        }
        .proDet p {
        padding-bottom: 3px;
        }
        .slds-media{
        padding-bottom: 0px;
        }
        .text-center{
        text-align: center;
        }
        .heightfixprint {
        width: 629px;
        margin-left: -26px;
        }
        .proDet{
        margin-bottom: 10px;
        margin-left: 50px;
        }
        .proDet p {
        padding-bottom: 3px;
        }
        
        .heightfixprint {
        width: 855px;
        margin-left: -31px;
        margin-top: -23px;
        }
        .cUtility .slds-modal__content {
        padding: 17px; 
        border-radius: 1px !important;
        }  
        .runtime_platform_actionsQuickActionWrapper .quick-actions-panel {
        overflow-y: auto;
        }

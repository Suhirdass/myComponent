    <rules>
        <fullName>Received Retail Ticket Approval from Retailer</fullName>
        <actions>
            <name>Received_Approval_for_Retail_Ticket</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Order__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Retailer Delivery Ticket</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order__c.Status__c</field>
            <operation>equals</operation>
            <value>Approved</value>
        </criteriaItems>
        <description>Alerts when Retailer Contact on RDT Approves via url click</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Resend Email Approval URL to Retail Contact after 48 and 72 Hours</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Order__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>Brand</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order__c.Status__c</field>
            <operation>equals</operation>
            <value>Draft</value>
        </criteriaItems>
        <criteriaItems>
            <field>Order__c.Total_Line_Items__c</field>
            <operation>greaterThan</operation>
            <value>0</value>
        </criteriaItems>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>X2nd_Attempt_Order_Approval_Reminder</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Order__c.CreatedDate</offsetFromField>
            <timeLength>2</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
        <workflowTimeTriggers>
            <actions>
                <name>X3rd_Attempt_Order_Approval_Reminder</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Order__c.CreatedDate</offsetFromField>
            <timeLength>4</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
</Workflow>

<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>UID_Character_Validation</fullName>
    <active>false</active>
    <errorConditionFormula>LEN(Name) &lt;&gt; 24  &amp;&amp; Name &lt;&gt; &apos;N/A&apos;</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Insufficient UID Character. Please check and reenter.</errorMessage>
</ValidationRule>

<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Production_Time__c</fullName>
    <externalId>false</externalId>
    <formula>(Actual_Completion_Date__c - Actual_Start_Date__c) * 24 * 60 - IF( Lunch_Included__c = True, 30, 0) - IF( Break_Included__c = True, 15, 0)</formula>
    <inlineHelpText>Production Time Expressed in Minutes</inlineHelpText>
    <label>Total Production Time</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Flag_for_closed_Status__c</fullName>
    <description>Flag used in PO workflow</description>
    <externalId>false</externalId>
    <formula>IF( (ISPICKVAL(POLI_Status__c, &apos;Closed&apos;) || ISPICKVAL(POLI_Status__c, &apos;Cancelled&apos;)), 0, 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Flag for closed Status</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>

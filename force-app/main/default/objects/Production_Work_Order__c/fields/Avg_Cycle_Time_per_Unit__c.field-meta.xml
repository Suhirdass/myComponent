    <inlineHelpText>Calculated; Expressed in seconds. Average cycle time per unit</inlineHelpText>
    <label>Time per Unit</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
